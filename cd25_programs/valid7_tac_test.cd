func printintpair(k : integer, j : integer) : void
begin
	Out << "First", k, " Second:", j << Line;
	return void;
end

func fibonacci(n : integer) : integer
	a : integer,
	b : integer,
	temp : integer,
	i : integer
begin
	a = 0;
	b = 1;
	for (i = 0; i < n)
		a += b;
		temp = a;
		a = b;
		b = temp;
		i += 1;
	end
	return a;
end

main
	w : real,
	x : integer,
	y : integer,
	z : integer
begin
	/-- float promotion and AST unfolding
	w = 1.5;
	y = 5;
	w = 0.0 + x + y + z;
	Out << w << Line;
	printintpair(1 + 2 * 3, fibonacci(fibonacci(6)));
end CD25 TACtest
